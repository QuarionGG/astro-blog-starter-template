  visibleGrid.style.transform =
    `translateX(calc((100vw - 100%) * ${pn})) translateY(calc(${pn} * var(--config-canvas-height) - ${pct}))`;
});






















// 4. SCALE & ROTATEY BASED ON SCROLL VELOCITY + TILT ON HOVER -------------------------------

// Mapping velocità → scale e rotateY
const scaleMap = transform([0, 2700], [1, 1.1]);              
const rotateYMap = transform([0, 3200], ['0deg', '-35deg']);  

// Tilt MotionValues per ogni showcase-slide-inner
const tiltXMVs = visibleShowcases.map(() => motionValue(0));
const tiltYMVs = visibleShowcases.map(() => motionValue(0));

const tiltSpringConfig = { stiffness: 150, damping: 12 };

// Pointer Events sulle orchestrator-slide-x (invisibili ma ricevono pointer events)
orchSlides.forEach((slide, idx) => {
  slide.addEventListener("pointermove", e => {
    const rect = slide.getBoundingClientRect();
    // Mappiamo da [-1,1] per ottenere ±8° effettivi
    const dx = ((e.clientX - rect.left) / rect.width - 0.5) * 2;
    const dy = ((e.clientY - rect.top) / rect.height - 0.5) * 2;

    tiltXMVs[idx].set(dx);
    tiltYMVs[idx].set(-dy); // Inverti per "spingere dentro"
  });

  slide.addEventListener("pointerleave", () => {
    tiltXMVs[idx].set(0);
    tiltYMVs[idx].set(0);
  });
});

