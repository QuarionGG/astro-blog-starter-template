    ensureYouTubeListening(ifr, force = false) {
      if (!ifr.contentWindow) return;
      if (this.ytReady.get(ifr) && !force) return;

      this.log('send "listening" to yt iframe');
      // The "listening" message gets the player to start posting messages back
      // We also set events we care about right away.
      const payloads = [
        { event: 'listening', id: 'OneMediaAtATime' },
        { event: 'command', func: 'addEventListener', args: ['onStateChange'] },
        { event: 'command', func: 'addEventListener', args: ['onReady'] }
