    const candidates = [
      document.querySelector('.root-slider'),
      document.documentElement,
      document.body
    ];
    for (const el of candidates) {
      if (!el) continue;
      const raw = getComputedStyle(el).getPropertyValue('--dynamic-active-slide-index');
      if (raw) {
        const n = parseInt(raw.trim(), 10);
        if (!Number.isNaN(n)) return n;
      }
    }
    return 0;
  }

  function applyActiveClass() {
    const orchWrapper = document.querySelector('.canvasWrapper._canvasOrchestratorWrapper');
    if (!orchWrapper) return;

    const slides = Array.from(orchWrapper.querySelectorAll('.orchestrator-slide-x'));
    if (!slides.length) return;

    let idx = readActiveIndex();
    if (idx < 0) idx = 0;
    if (idx >= slides.length) idx = slides.length - 1;

    slides.forEach((el, i) => {
      el.classList.toggle('orchestrator-slide-active', i === idx);
    });
  }

  applyActiveClass();
})();
</script>

