 * OneMediaAtATime — global coordinator
 * Ensures only one media plays at a time across:
 *   - <audio> elements
 *   - <video> elements
 *   - YouTube iframes (youtube.com or youtube-nocookie.com with enablejsapi=1)
 *
 * Works with your gallery: listens for "videoGallery:iframeLoaded" and registers the new iframe.
 * Also auto-detects future media via a MutationObserver.
 */
(function () {
  const One = {
    DEBUG: false, // flip off when done
    log(...a)  { if (this.DEBUG) console.log('[OneMedia]', ...a); },
    warn(...a) { if (this.DEBUG) console.warn('[OneMedia]', ...a); },

    audioEls: new Set(),
    videoEls: new Set(),
    ytIframes: new Set(),
    ytReady: new WeakMap(),   // iframe -> boolean (we sent "listening")
    ytIds:   new WeakMap(),   // iframe -> string id (for logs)

    init() {
      this.log('init');
      this.scanAndRegisterAll();
      this.observeDom();
      this.wireYouTubeMessageBus();

      // When your gallery swaps in a fresh iframe, register it and pause everything else.
      document.addEventListener('videoGallery:iframeLoaded', () => {
        this.log('event: videoGallery:iframeLoaded');
        const fresh = this.findYouTubeIframes();
        fresh.forEach((ifr) => this.registerYouTubeIframe(ifr));
        // treat this as “YouTube started” → pause other media
        const active = this.getMostRecentIframe() || null;
        this.pauseAllExcept({ kind: 'youtube', el: active });
      });

      // Also if your gallery changes active video and already has an iframe playing
      document.addEventListener('videoGallery:activeChanged', () => {
        this.log('event: videoGallery:activeChanged');
        const active = this.getMostRecentIframe() || null;
        if (active) this.pauseAllExcept({ kind: 'youtube', el: active });
      });
    },

    // ——— Scanning & registration ———
    scanAndRegisterAll() {
      this.log('scanAndRegisterAll');
      this.findAudios().forEach((el) => this.registerAudio(el));
      this.findVideos().forEach((el) => this.registerVideo(el));
      this.findYouTubeIframes().forEach((ifr) => this.registerYouTubeIframe(ifr));
    },

    findAudios() {
      return Array.from(document.querySelectorAll('audio'));
    },
    findVideos() {
      return Array.from(document.querySelectorAll('video'));
    },
    isYouTubeIframe(ifr) {
      if (!(ifr instanceof HTMLIFrameElement)) return false;
      const src = (ifr.getAttribute('src') || '').toLowerCase();
      return /youtube\.com\/embed|youtube\-nocookie\.com\/embed/.test(src);
    },
    findYouTubeIframes() {
      return Array.from(document.querySelectorAll('iframe')).filter((ifr) => this.isYouTubeIframe(ifr));
    },

    registerAudio(el) {
      if (this.audioEls.has(el)) return;
      this.audioEls.add(el);
      this.log('registerAudio', el);

      el.addEventListener('play', () => {
        this.log('audio PLAY', el);
        this.pauseAllExcept({ kind: 'audio', el });
      }, { passive: true });

