// 3) aggiorna i target su cambi di velocità springata
springedVelocityMV.on("change", v => {
  const speedRatio = Math.min(v, maxV) / maxV;  // [0…1]
  const activeIdx  = activeSlideMV.get();

  dropShadows.forEach((el, i) => {
    if (!el) return;
    let targetX;

    if ( i < activeIdx) {
      // SOLO su mobile, slide “a sinistra” vanno sempre a mobileBaseline
      targetX = ShadowLeftBaseline; // –40
    } else {
      // tutte le altre (desktop e mobile) → 0→maxDropDesktop * speedRatio
      targetX = speedRatio * maxDropDesktop;
    }

    // aggiorna il MotionValue che poi animerà con spring
    dropXVals[i].set(targetX);
  });
});














// ——— Assicuriamoci di azzerare velocityMV quando il progress si ferma ———
let scrollStopTimeout;

springedProgressMV.on("change", () => {
  // se stiamo ancora drag‐gando, non resettiamo la velocity
  if (isDragging) return;

  clearTimeout(scrollStopTimeout);
  // dopo 150ms di inattività, azzeriamo la velocity
  scrollStopTimeout = setTimeout(() => {
    velocityMV.set(0);
  }, 0);
});




});
</script>


