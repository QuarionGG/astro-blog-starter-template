    convergeWrapper[i].style.transform = `translateX(${x.toFixed(2)}px)`;
  });
});


springedVelocityMV.on("change", v => {
  const speedRatio = Math.min(v, maxV) / maxV;
  const gridRect    = visibleGrid.getBoundingClientRect();
  const gridCenterX = gridRect.left + gridRect.width / 2;
  const isMobile    = window.innerWidth <= mobileBp;
  const activeIndex = activeSlideMV.get();

  

  convergeWrapper.forEach((wrapper, i) => {
    // calcola centro di ciascuna slide
    const wrapRect    = wrapper.getBoundingClientRect();
    const wrapCenterX = wrapRect.left + wrapRect.width / 2;

    let target;
    if (isMobile && i < activeIndex && v < 1000) {
      // branch 1: mobile & left-of-active → baseline
      target = mobileBaseline;
    } else {
      // branch 2: converge generica
      const dirPx   = gridCenterX - wrapCenterX;
      const dirNorm = dirPx / (gridRect.width / 2); // [-1…1]
      const maxOff  = isMobile ? maxOffsetM : maxOffsetD;
      target = dirNorm * maxOff * speedRatio;
    }

    // **QUI** non usiamo style.transform diretto,
    // ma settiamo il MotionValue: lo spring poi anima verso `target`
    translateXVals[i].set(target);
  });


