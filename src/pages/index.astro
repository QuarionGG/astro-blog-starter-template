      // If iframe reloads, re-send listening
      ifr.addEventListener('load', () => {
        this.log('iframe load → re-init listening');
        this.ensureYouTubeListening(ifr, true);
      });
    },

    extractYouTubeIdFromSrc(src) {
      try {
        const u = new URL(src, location.href);
        const m = u.pathname.match(/\/embed\/([\w-]{11})/);
        return m ? m[1] : '';
      } catch { return ''; }
    },

