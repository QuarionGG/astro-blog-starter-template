visibleShowcases.forEach((showEl, idx) => {
  const springTiltX = springValue(tiltXMVs[idx], tiltSpringConfig);
  const springTiltY = springValue(tiltYMVs[idx], tiltSpringConfig);

  const updateTransform = () => {
    const s = scaleMap(springedVelocityMV.get());
    const rYScroll = rotateYMap(springedVelocityMV.get());

    const tiltX = springTiltX.get() * 8;  // ±8° tilt
    const tiltY = springTiltY.get() * 8;

    showEl.style.transform = `
      scale(${s})
      rotateY(${rYScroll})
      rotateX(${tiltY}deg)
      rotateY(${tiltX}deg)
    `;
  };

  // Subscriptions → trigger update su tutti i cambiamenti rilevanti
  springedVelocityMV.on("change", updateTransform);
  springTiltX.on("change", updateTransform);
  springTiltY.on("change", updateTransform);
  parallaxY.on("change", updateTransform);
});














// FUNCTION CONVERGE EFFECT


// subito dopo aver raccolto `visibleWrappers` e `activeSlideMV`:
const translateXVals    = convergeWrapper.map(() => motionValue(0));
const translateXSprings = translateXVals.map(v => 
