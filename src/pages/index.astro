          return;
        }

        // infoDelivery: player state changes etc.
        if (event === 'infoDelivery' && info && typeof info.playerState === 'number') {
          // 1 = playing, 2 = paused, per YT Iframe API
          if (info.playerState === 1) {
            // find which iframe it is (best-effort: event origin not guaranteed;
            // many implementations include "id" but we also just pause others anyway)
            this.log('YT state = PLAYING from', id || '(unknown)');
            // We don’t know exact element from this message always.
            // Safer: pause everything except ALL YT iframes (we’ll assume the one that just started should keep playing).
            // But this could keep multiple YT iframes playing at once.
            // Better: if we can find the active/most-recent iframe, use that.
            const active = this.getMostRecentIframe();
            this.pauseAllExcept({ kind: 'youtube', el: active });
          }
        }
      });
    },

    getMostRecentIframe() {
      // Best-effort: the last YouTube iframe in DOM is usually the fresh one (your gallery replaces the placeholder with it)
      const arr = Array.from(this.ytIframes).filter(ifr => document.documentElement.contains(ifr));
      return arr[arr.length - 1] || null;
    },

    // ——— Pause logic ———
    pauseAllExcept({ kind, el }) {
      this.log('pauseAllExcept', { kind, el });

      // Pause all audios
      this.audioEls.forEach((a) => {
        if (!(kind === 'audio' && a === el)) {
          if (!a.paused) {
            this.log('pausing <audio>', a);
            try { a.pause(); } catch {}
          }
        }
      });

      // Pause all HTML5 videos
      this.videoEls.forEach((v) => {
        if (!(kind === 'video' && v === el)) {
          if (!v.paused) {
            this.log('pausing <video>', v);
            try { v.pause(); } catch {}
          }
        }
      });

      // Pause all YouTube iframes (except the chosen one, if provided)
      this.ytIframes.forEach((ifr) => {
        if (!(kind === 'youtube' && ifr === el)) {
          this.pauseYouTube(ifr);
        }
      });
    },

    pauseYouTube(ifr) {
      if (!ifr || !this.isYouTubeIframe(ifr) || !ifr.contentWindow) return;
      const id = this.ytIds.get(ifr) || '(no-id)';
      this.log('pauseYouTube', id, ifr);

      // Send pauseVideo a few times in case API not fully ready yet.
      const send = () => {
        try {
          ifr.contentWindow.postMessage(JSON.stringify({
            event: 'command',
            func: 'pauseVideo',
            args: []
          }), '*');
        } catch {}
      };
