<script>document.addEventListener('DOMContentLoaded', function(){ 
(function() {

  // ─────────── 1) CONFIG ───────────
  const PLAY_SPEED_DIVISOR = 400;
  const HOVER_DURATION     = 1500;
  const RESET_DELAY        = 100;

  // ─────────── 2) ELEMENTS ───────────
  const mainSec = document.querySelector('.qg-main-section');
  const button  = document.querySelector('.soundclip.soundclip_b');
  if (!mainSec || !button) return;

  const audio     = button.querySelector('audio');
  const sourceEl  = audio.querySelector('source');
  const playIcon  = button.querySelector('.playIcon');
  const pauseIcon = button.querySelector('.pauseIcon');

  let rafId        = null;
  let isPlaying    = false;
  let hoverTimeout = null;
  let phaseOffsets = [];

  // ─────────── 3) HELPERS ───────────
  function getAudioUrl() {
    const wrapper = document.querySelector('._soundclip-music-wrapper');
    if (wrapper) {
      return wrapper.getAttribute('audioclip-link') || '';
    }
    const slide = getActiveSlide();
    return slide?.getAttribute('audioclip-link') || '';
  }

  function resetLines() {
    button.querySelectorAll('svg line').forEach(l => {
      l.style.transform = 'none';
    });
  }

  function initPhases() {
    const lines = button.querySelectorAll('svg line');
    phaseOffsets = Array.from(lines).map(() => Math.random() * Math.PI * 2);
  }

  function continuousAnimate() {
    const lines = button.querySelectorAll('svg line');
    const t     = performance.now() / PLAY_SPEED_DIVISOR;
    lines.forEach((line, i) => {
      const phase = t + phaseOffsets[i];
      const ratio = 1 + Math.sin(phase) * 0.5;
      line.style.transformOrigin = 'center';
      line.style.transform       = `scaleY(${ratio.toFixed(3)})`;
    });
    rafId = requestAnimationFrame(continuousAnimate);
  }

  function getActiveSlide() {
    let slide = document.querySelector('.orchestrator-slide-x.orchestrator-slide-active');
    if (!slide) {
      const idx = parseInt(
        getComputedStyle(mainSec)
          .getPropertyValue('--dynamic-active-slide-index').trim(),
        10
      );
      slide = document.querySelectorAll('.orchestrator-slide-x')[idx];
    }
    return slide;
  }

  function updateClip() {
    const url = getAudioUrl();
    if (!url) return;

    if (sourceEl.src !== url) {
      sourceEl.src = url;
      audio.load();
      if (button.classList.contains('playing')) {
        audio.play().catch(() => {});
      } else {
        playIcon.style.opacity  = '1';
        pauseIcon.style.opacity = '0';
        resetLines();
      }
    }
  }

  // ─────────── 4) SLIDE OBSERVER ───────────
  let lastIndex = getComputedStyle(mainSec)
                    .getPropertyValue('--dynamic-active-slide-index').trim();

  const slideObserver = new MutationObserver(mutations => {
    for (const m of mutations) {
      if (
        m.type === 'attributes' &&
        m.target === mainSec &&
        m.attributeName === 'style'
      ) {
        const newIndex = getComputedStyle(mainSec)
                           .getPropertyValue('--dynamic-active-slide-index').trim();
        if (newIndex !== lastIndex) {
          lastIndex = newIndex;
          updateClip();
        }
      }
      if (
        m.type === 'attributes' &&
        m.attributeName === 'class' &&
        m.target.classList.contains('orchestrator-slide-x')
      ) {
        const wasActive = m.oldValue && m.oldValue.includes('orchestrator-slide-active');
        const isActive  = m.target.classList.contains('orchestrator-slide-active');
        if (!wasActive && isActive) {
          updateClip();
        }
      }
    }
  });

  slideObserver.observe(mainSec, {
    attributes: true,
    attributeFilter: ['style','class'],
    attributeOldValue: true,
    subtree: true
  });

  const altWrapper = document.querySelector('._soundclip-music-wrapper');
  if (altWrapper) {
    new MutationObserver(muts => {
      muts.forEach(m => {
        if (m.type === 'attributes' && m.attributeName === 'audioclip-link') {
          updateClip();
        }
      });
    }).observe(altWrapper, { attributes: true, attributeFilter: ['audioclip-link'] });
  }

  // ─────────── 5) AUDIO EVENTS ───────────
  audio.addEventListener('play', () => {
    isPlaying = true;
    playIcon.style.opacity  = '0';
    pauseIcon.style.opacity = '1';
    button.classList.add('playing');
    cancelAnimationFrame(rafId);
    continuousAnimate();
  });

  audio.addEventListener('pause', () => {
    isPlaying = false;
    cancelAnimationFrame(rafId);
    setTimeout(() => {
      if (!button.matches(':hover')) resetLines();
    }, RESET_DELAY);
    playIcon.style.opacity  = '1';
    pauseIcon.style.opacity = '0';
    button.classList.remove('playing');
  });

  audio.addEventListener('ended', () => {
    button.classList.remove('playing');
  });

  // ─────────── 6) HOVER BURSTS ───────────
  button.addEventListener('mouseenter', () => {
    if (audio.paused) {
      cancelAnimationFrame(rafId);
      continuousAnimate();
      clearTimeout(hoverTimeout);
      hoverTimeout = setTimeout(() => {
        cancelAnimationFrame(rafId);
        if (!button.matches(':hover')) resetLines();
      }, HOVER_DURATION);
    }
  });

  button.addEventListener('mouseleave', () => {
    clearTimeout(hoverTimeout);
    if (audio.paused) {
      cancelAnimationFrame(rafId);
      setTimeout(resetLines, RESET_DELAY);
    }
  });

  // ─────────── 7) CLICK PLAY/PAUSE ───────────
  button.addEventListener('click', () => {
    if (audio.paused) {
      audio.play().catch(() => {});
    } else {
      audio.pause();
    }
  });

  // ─────────── 8) INIT ───────────
  function init() {
    initPhases();
    updateClip();
  }

  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', init);
  } else {
    init();
  }
})();
 }) </script>
