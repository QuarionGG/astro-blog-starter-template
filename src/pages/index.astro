  <script>
  (function () {
    const DEBUG = false; // flip off once verified

    const EASE = [0.1, 0.9, 0.2, 1];
    const HEADER_SELECTOR = 'header._root-header';

    // ----- tiny helpers -----
    function log(...a){ if (DEBUG) console.log("[StickyHeader]", ...a) }
    function warn(...a){ console.warn("[StickyHeader]", ...a) }

    function waitFor(cond, label, timeout = 4000, poll = 20) {
      return new Promise((resolve, reject) => {
        const start = performance.now();
        (function tick(){
          if (cond()) return resolve();
          if (performance.now() - start > timeout) {
            return reject(new Error(label + " timed out"));
          }
          setTimeout(tick, poll);
        })();
      });
    }

    function onMOReady() {
      return new Promise((resolve) => {
        if (window.MO) return resolve(window.MO);
        document.addEventListener("MOReady", (e) => resolve(e.detail), { once: true });
      });
    }

    async function boot() {
      log("Booting…");

      // 1) Wait for DOM
      if (document.readyState === "loading") {
        await new Promise(r => document.addEventListener("DOMContentLoaded", r, { once: true }));
        log("DOMContentLoaded");
      }

      // 2) Wait for MO (race: event or polling fallback)
      const mo = await Promise.race([
        onMOReady(),
        waitFor(() => !!window.MO, "MO shim", 3000).then(() => window.MO)
      ]).catch(err => {
        warn("Motion (MO) not found after wait.", err.message);
        return null;
      });

      if (!mo) return; // bail early, log already printed

      // 3) Verify required APIs exist
      const needs = ["scroll", "resize", "animate"];
      const missing = needs.filter(k => !(k in mo));
      log("MO keys:", Object.keys(mo));
      if (missing.length) warn("Missing Motion APIs:", missing);

      // 4) Grab header (retry briefly in case it’s rendered late)
      let header = document.querySelector(HEADER_SELECTOR);
      if (!header) {
        warn(`No ${HEADER_SELECTOR} found on first try, retrying…`);
        try {
          await waitFor(() => (header = document.querySelector(HEADER_SELECTOR)), "header", 1500);
        } catch {
          warn(`No ${HEADER_SELECTOR} found. Aborting.`);
          return;
        }
      }

      // ----- state & prefs -----
      const prefersReduced = matchMedia('(prefers-reduced-motion: reduce)').matches;
      const state = { mode: "relative", visible: false };
      let headerHeight = header.offsetHeight;

      log("Header found. Initial height:", headerHeight, "Reduced motion:", prefersReduced);

      // Keep threshold up to date
      if (mo.resize) {
        mo.resize(header, (_, { height }) => {
          headerHeight = height;
          log("Updated header height:", height);
        });
      } else {
        warn("resize() unavailable – height won’t auto-update.");
      }

      // ----- helpers -----
      function setRelative() {
        if (state.mode === "relative") { log("Already relative; skip"); return; }
        header.style.position = "relative";
        header.style.top = "";
        header.style.transform = "";
        header.dataset.stickyVisible = "false";
        state.mode = "relative";
        state.visible = false;
        log("Set mode → relative");
      }

      function ensureSticky() {
        if (state.mode !== "sticky") {
          header.style.position = "sticky";
          header.style.top = "0px";
          state.mode = "sticky";
          log("Set mode → sticky");
        }
      }

      function setStickyHidden() {
        ensureSticky();
        header.dataset.stickyVisible = "false";
        if (state.visible && !prefersReduced && mo.animate) {
          log("Animating hide");
          mo.animate(header, { y: ["0%", "-100%"] }, { duration: 0.3, ease: EASE });
        } else {
          header.style.transform = "translateY(-100%)";
          log("Hidden instantly");
        }
        state.visible = false;
      }

      function setStickyVisible() {
        ensureSticky();
        header.dataset.stickyVisible = "true";
        if (!state.visible && !prefersReduced && mo.animate) {
          log("Animating show");
          mo.animate(header, { y: ["-100%", "0%"] }, { duration: 0.3, ease: EASE });
        } else {
          header.style.transform = "translateY(0%)";
          log("Shown instantly");
        }
        state.visible = true;
      }

      // ----- unified scroll handler (hide on down, show on up) -----
      function onScrollInfo(y, vy) {
        if (DEBUG) log("y:", y, "vy:", vy);

        if (y <= 0) return setRelative();

        if (y > headerHeight) {
          if (vy < 0)       return setStickyVisible(); // up
          else if (vy > 0)  return setStickyHidden();  // down
          // vy === 0 → do nothing
        } else {
          setRelative();
        }
      }

      // ----- bind exactly one listener -----
      let stopScroll;
      if (mo.scroll) {
        log("Binding Motion scroll listener…");
        stopScroll = mo.scroll((progress, info) => {
          const y  = info?.y?.current  ?? window.scrollY ?? 0;
          const vy = info?.y?.velocity ?? 0;
          onScrollInfo(y, vy);
        });
      } else {
        warn("scroll() unavailable – using passive native scroll as fallback.");
        const handler = () => {
          const y = window.scrollY || 0;
          const now = performance.now();
          const dy = y - (handler._py || 0);
          const dt = now - (handler._pt || now - 16);
          handler._py = y; handler._pt = now;
          const vy = dt ? (dy / (dt / 1000)) : 0; // px/s
          onScrollInfo(y, vy);
        };
        window.addEventListener("scroll", handler, { passive: true });
        handler(); // initial
        stopScroll = () => window.removeEventListener("scroll", handler);
      }

      // Expose a tiny dev hook to unbind if needed
      window.__StickyHeaderStop = stopScroll;
      log("Ready ✔");
    }

    // Kick off
    boot().catch(err => warn("Boot error:", err));
  })();
