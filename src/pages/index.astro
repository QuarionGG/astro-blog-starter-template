    },

    registerVideo(el) {
      if (this.videoEls.has(el)) return;
      this.videoEls.add(el);
      this.log('registerVideo', el);

      el.addEventListener('play', () => {
        this.log('video PLAY', el);
        this.pauseAllExcept({ kind: 'video', el });
      }, { passive: true });
    },

    registerYouTubeIframe(ifr) {
      if (!this.isYouTubeIframe(ifr)) return;
      if (this.ytIframes.has(ifr)) return;
      this.ytIframes.add(ifr);

      // Optional id for logging
      const id = this.extractYouTubeIdFromSrc(ifr.getAttribute('src') || '');
