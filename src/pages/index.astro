            node.querySelectorAll && node.querySelectorAll('audio,video,iframe').forEach(el => {
              if (el.tagName === 'AUDIO') { this.registerAudio(el); added = true; }
              if (el.tagName === 'VIDEO') { this.registerVideo(el); added = true; }
              if (el.tagName === 'IFRAME' && this.isYouTubeIframe(el)) { this.registerYouTubeIframe(el); added = true; }
            });
          });
        });
