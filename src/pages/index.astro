  // Candidate API list (everything except animateView & splitText)
  const candidates = [
    // Core
    "animate", "scroll",

    // Gestures
    "hover", "inView", "press", "resize",

    // Motion values
    "motionValue", "mapValue", "transformValue", "springValue",

    // Utils
    "delay", "frame", "cancelFrame", "mix", "spring", "stagger", "transform", "wrap",

    // Effects (may be missing depending on build)
    "attrEffect", "propEffect", "styleEffect", "svgEffect",

    // Easings
    "easeIn", "easeOut", "easeInOut",
    "backIn", "backOut", "backInOut",
    "circIn", "circOut", "circInOut",
    "anticipate", "linear", "steps",
  ];

  // Build MO namespace with only available keys
  const MO = Object.freeze(
    Object.fromEntries(
      candidates
        .filter((key) => key in Motion)
        .map((key) => [key, Motion[key]])
    )
  );

  // Install into window
  if (!("MO" in window)) {
    Object.defineProperty(window, "MO", { value: MO, writable: false, configurable: false });
  }
  if (!("Motion12" in window) && MO.animate) {
    Object.defineProperty(window, "Motion12", {
      value: Object.freeze({ animate: MO.animate }),
      writable: false,
      configurable: false,
    });
  }

  // Diagnostics (only if DEBUG = true)
  if (DEBUG) {
    const missing = candidates.filter((key) => !(key in Motion));
    console.groupCollapsed("%c[Motion 12 Import Report]", "color: dodgerblue; font-weight: bold;");
    console.info("✅ Available:", Object.keys(MO));
    if (missing.length) console.warn("⚠️ Missing exports:", missing);
    console.groupEnd();
  }

